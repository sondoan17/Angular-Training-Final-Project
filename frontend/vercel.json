{
  "version": 2,
  "builds": [
    {
      "src": "dist/angular-training-final-project/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(assets|favicon.ico)/(.*)",
      "dest": "/dist/angular-training-final-project/$1/$2"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/angular-training-final-project/index.html"
    }
  ]
}